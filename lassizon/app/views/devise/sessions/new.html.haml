%h2 Log In
= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f|
  .field
    = f.label :email
    %br/
    = f.email_field :email, autofocus: true, autocomplete: "email", id:"loginemail", disabled: false
  .field
    = f.label :password
    %br/
    = f.password_field :password, autocomplete: "current-password", id:"loginpassword"
  - if devise_mapping.rememberable?
    .field
      = f.check_box :remember_me
      = f.label :remember_me
  .actions
    = f.submit "Log in", id:"loginbutton"
= render "devise/shared/links"
